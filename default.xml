<?xml version="1.0" encoding="UTF-8"?>
<!--
   Custom manifest for CAmkES VM Examples with vm_freertos integration
   Based on upstream seL4/camkes-vm-examples-manifest
-->
<manifest>
  <remote name="lwip" fetch="https://git.savannah.gnu.org/git/"/>
  <remote name="picotcp" fetch="https://github.com/tass-belgium"/>
  <remote name="polly" fetch="https://github.com/ruslo"/>
  <remote name="seL4" fetch="https://github.com/seL4"/>
  <remote name="zeromq" fetch="https://github.com/zeromq"/>
  <remote name="spanwich" fetch="https://github.com/spanwich"/>
  
  <default remote="seL4" revision="master"/>
  
  <!-- Core CAmkES and seL4 components -->
  <project name="camkes-tool.git" path="camkes-vm-examples/projects/camkes-tool" revision="7ce8ef4ef82e51461b0dd4d169c3c8aaa1b7b9c4" upstream="master" dest-branch="master">
    <linkfile src="docs/index.md" dest="camkes-vm-examples/camkes_README.md"/>
  </project>
  
  <!-- VM Examples - using your fork with Claude Code build improvements -->
  <project name="camkes-vm-examples.git" path="camkes-vm-examples/projects/vm-examples" remote="spanwich" revision="master" upstream="master" dest-branch="master">
    <linkfile src="easy-settings.cmake" dest="camkes-vm-examples/easy-settings.cmake"/>
  </project>
  
  <!-- Custom vm_freertos project from your GitHub -->
  <project name="camkes-vm-freertos.git" path="camkes-vm-examples/projects/vm-examples/apps/Arm/vm_freertos" 
           remote="spanwich" revision="main" upstream="main" dest-branch="main"/>

  <!-- FreeRTOS source for ARM Cortex-A9 vExpress platform -->
  <project name="freertos_vexpress_a9.git" path="freertos_vexpress_a9" 
           remote="spanwich" revision="master" upstream="master" dest-branch="master"/>
           
  <!-- Standalone FreeRTOS QEMU test project -->
  <project name="freertos-qemu-research.git" path="freertos_qemu" 
           remote="spanwich" revision="master" upstream="master" dest-branch="master"/>
  
  <!-- VM Images and Linux -->
  <project name="camkes-vm-images.git" path="camkes-vm-examples/projects/camkes-vm-images" revision="bafad64282c14d0bbf15468bfd6265e648d87d6c" upstream="master" dest-branch="master"/>
  <project name="camkes-vm-linux.git" path="camkes-vm-examples/projects/vm-linux" revision="608ff1861ad3b9ebccbe3576dab5777691fca1f6" upstream="master" dest-branch="master"/>
  <project name="camkes-vm.git" path="camkes-vm-examples/projects/vm" remote="spanwich" revision="master" upstream="master" dest-branch="master">
    <linkfile src="README.md" dest="camkes-vm-examples/README.md"/>
  </project>
  
  <!-- Core libraries and tools -->
  <project name="capdl.git" path="camkes-vm-examples/projects/capdl" remote="spanwich" revision="master" upstream="master" dest-branch="master"/>
  <project name="global-components.git" path="camkes-vm-examples/projects/global-components" revision="e4fbff83ce2998c93c24d59d8b4e0f6c3ff840d8" upstream="master" dest-branch="master"/>
  <project name="libzmq" path="camkes-vm-examples/projects/libzmq" remote="zeromq" revision="d062edd8c142384792955796329baf1e5a3377cd" upstream="refs/tags/v4.2.5" dest-branch="refs/tags/v4.2.5"/>
  <project name="lwip.git" path="camkes-vm-examples/projects/lwip" remote="spanwich" revision="159e31b689577dbf69cf0683bbaffbd71fa5ee10" upstream="refs/tags/STABLE-2_1_2_RELEASE" dest-branch="refs/tags/STABLE-2_1_2_RELEASE"/>
  <project name="musllibc.git" path="camkes-vm-examples/projects/musllibc" revision="c9249738defe1ddd14f7d4b634dc5d945e7d00bb" upstream="sel4" dest-branch="sel4"/>
  <project name="picotcp.git" path="camkes-vm-examples/projects/picotcp" remote="picotcp" revision="13c00a067190f5ec62340ed932a2bb833c366d63" upstream="refs/tags/v1.7.0" dest-branch="refs/tags/v1.7.0"/>
  <project name="polly" path="camkes-vm-examples/tools/polly" remote="polly" revision="ef7e79c2c297d456f2742fd0b976f555d058d4e0" upstream="master" dest-branch="master"/>
  <project name="projects_libs.git" path="camkes-vm-examples/projects/projects_libs" revision="1e016fb78ca28a3c50232a93911a3c6eb79a0d40" upstream="master" dest-branch="master"/>
  
  <!-- seL4 kernel and libraries - using forked version for capability debugging -->
  <project name="seL4" path="camkes-vm-examples/kernel" revision="master" upstream="master" dest-branch="master" remote="spanwich"/>
  <project name="seL4_libs" path="camkes-vm-examples/projects/seL4_libs" revision="2580739bde15ec0dac1bef43a4e313e3752b7692" upstream="master" dest-branch="master"/>
  <project name="seL4_projects_libs.git" path="camkes-vm-examples/projects/seL4_projects_libs" remote="spanwich" revision="master" upstream="master" dest-branch="master"/>
  <project name="seL4_tools.git" path="camkes-vm-examples/tools/seL4" revision="ee550199fa02eefa73732935f0e8f6003a09a3b9" upstream="master" dest-branch="master">
    <linkfile src="cmake-tool/griddle" dest="camkes-vm-examples/griddle"/>
    <linkfile src="cmake-tool/init-build.sh" dest="camkes-vm-examples/init-build.sh"/>
  </project>
  
  <!-- Custom build script for Claude Code compatibility -->
  <!-- Disabled: path="." causes repo sync conflicts
  <project name="phd.git" path="." remote="spanwich" revision="master" upstream="master" dest-branch="master">
    <linkfile src="claude-build.sh" dest="camkes-vm-examples/claude-build.sh"/>
  </project>
  -->
  <project name="sel4runtime" path="camkes-vm-examples/projects/sel4runtime" revision="86489cf6efab9f314964e79468c036e9035394c7" upstream="master" dest-branch="master"/>
  <project name="util_libs.git" path="camkes-vm-examples/projects/util_libs" revision="1ba664001364c57599dfcda707adec16ee646bb5" upstream="master" dest-branch="master"/>
</manifest>